{
  "title": "right shift vi like",
  "rules": [
    {
      "description": "Right shift to hyper (right_shift to hyper)",
      "manipulators": [
        {
          "from": {
            "key_code": "right_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "modifiers": [
                "right_control",
                "right_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Capslock to left command (caps_lock to left_command)",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Capslock and left control to hyper (caps_lock+left_control to hyper)",
      "manipulators": [
        {
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_command",
              "modifiers": [
                "right_control",
                "right_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Left command and space to escape (left_command+space to escape)",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Left command and return to spotlight (left_command+return to left_command+spacebar)",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
